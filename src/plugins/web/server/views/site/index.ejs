                <!--Case Section-->
                <section class="case-section">
                    <div class="auto-container">
                        <!--Sec Title-->
                        <div class="sec-title centered">
                            <h2>Nosso Produto</h2>
                        </div>
                    </div>
                    <!--Four Item Carousel-->
                    <div class="four-item-carousel owl-carousel owl-theme">


                        <% produtos.forEach(function(produto) { %>
                            <div class="case-block">
                                <div class="inner-box">
                                    <div class="image">
                                        <img src="<%= produto.foto?produto.foto.url:'/images/resource/case-1.jpg' %>" alt="" />
                                        <div class="overlay-box">
                                            <div class="overlay-inner">
                                                <div class="content">
                                                    <h3>
                                                        <a href="/galerias">
                                                            <%= produto.nome %>
                                                        </a>
                                                    </h3>
                                                    <div class="text">
                                                        <%= produto.descricao %>
                                                    </div>
                                                    <a href="/galerias" class="read-more">Ler mais <span class="fa fa-long-arrow-right"></span></a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <% }); %>



                    </div>
                    <!--Lower Box-->
                    <div class="lower-box">
                        <div class="case-text">Quer fazer um pedido? <a href="/contatos">entre em contato conosco.</a> Nosso especialista está pronto para ajudá-lo.</div>
