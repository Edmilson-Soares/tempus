<%- include('head/head.ejs'); %>

    <body class="hidden-bar-wrapper">

        <div class="page-wrapper">



            <!-- Main Header-->
            <%- include('header/header_início.ejs'); %>
                <!--End Main Header -->



                <!--Main Slider-->
                <section class="main-slider">

                    <div class="main-slider-carousel owl-carousel owl-theme">


                        <% destaques.forEach(function(destaque) { %>
                            <!--style="<%=//'background-image:'+'url('+destaque.foto?produto.foto.url:'images/main-slider/image-1.jpg'+')'%>"-->

                            <div class="slide" style="background-image:url(<%= destaque.foto?destaque.foto.url:'images/main-slider/image-1.jpg'%>) ;">
                                <div class="auto-container">
                                    <div class="content clearfix">
                                        <div class="title">
                                            <%= destaque.titulo1%>
                                        </div>
                                        <h1>
                                            <%= destaque.titulo%>
                                        </h1>
                                        <div class="text">
                                            <%= destaque.descricao%>
                                        </div>
                                        <div class="link-box">
                                            <a href="/sobre" class="theme-btn btn-style-one">Ler mais</a> <a href="/contatos" class="theme-btn btn-style-two">Contate-nos</a>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <% }); %>



