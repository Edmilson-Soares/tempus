                                                <a href="<%= parceria.link %> "><img src="<%= parceria.logo?parceria.logo.url:'/images/resource/logo/CeDRI2.png' %>" alt="<%= parceria.nome %>"></a>
                                            </div>
                                        </li>
                                        <% }); %>

                                </ul>
                            </div>
                        </div>
                    </section>
                    <!--End Sponsors Section-->

                    <!--News Section-->
