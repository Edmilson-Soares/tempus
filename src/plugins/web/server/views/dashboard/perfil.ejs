<head>
    <meta charset="UTF-8">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, shrink-to-fit=no" name="viewport">
    <title>Tempus</title>
    <!-- General CSS Files -->
    <link rel="stylesheet" href="/assets/css/app.min.css">
    <!-- Template CSS -->
    <link rel="stylesheet" href="/assets/css/style.css">
    <link rel="stylesheet" href="/assets/css/components.css">
    <!-- Custom style CSS -->
    <link rel="stylesheet" href="/assets/css/custom.css">
    <link rel="stylesheet" href="/assets/bundles/izitoast/css/iziToast.min.css">
    <link rel='shortcut icon' type='image/png' href='/assets/img/AddedSolutions.jpg' />
    <link rel="stylesheet" href="/assets/bundles/datatables/datatables.min.css">
    <link rel="stylesheet" href="/assets/bundles/datatables/DataTables-1.10.16/css/dataTables.bootstrap4.min.css">
</head>

<body>

    <div id="app">
        <div class="main-wrapper main-wrapper-1">
            <div class="navbar-bg"></div>
            <nav class="navbar navbar-expand-lg main-navbar sticky">
                <div class="form-inline mr-auto">
                    <ul class="navbar-nav mr-3">
                        <li>
                            <a href="#" data-toggle="sidebar" class="nav-link nav-link-lg
									collapse-btn"> <i data-feather="align-justify"></i></a>
                        </li>
                        <li>
                            <a href="#" class="nav-link nav-link-lg fullscreen-btn">
                                <i data-feather="maximize"></i>

                            </a>
                        </li>
                        <li>
                            <a class="nav-link nav-link-lg font-18">
                            <img src="<%= user.instituicao.logo.url %>" style="width: 100%;height: 40px;"  alt="" srcset="">


                            </a>
                        </li>
                        <li>
                            <a class="nav-link nav-link-lg font-18">

                                <%= user.instituicao.nome %>
                            </a>
                        </li>

                    </ul>
                </div>
                <ul class="navbar-nav navbar-right">
                    <li class="dropdown"><a href="#" data-toggle="dropdown" class="nav-link dropdown-toggle nav-link-lg nav-link-user"> <img alt="image" src="<%= user.foto?user.foto.url:'/assets/img/AddedSolutions.jpg' %> "
                class="user-img-radious-style"> <span class="d-sm-none d-lg-inline-block"></span></a>
                        <div class="dropdown-menu dropdown-menu-right pullDown">
                            <div class="dropdown-title">Olá
                                <%= user.username %>
                            </div>
                            <a href="/dashboard/perfil" class="dropdown-item has-icon"> <i class="far
										fa-user"></i> Perfil
                            </a>
                            <div class="dropdown-divider"></div>
                            <a href="/web/auth/logout" class="dropdown-item has-icon text-danger"> <i class="fas fa-sign-out-alt"></i> Sair
                            </a>
                        </div>
                    </li>
                </ul>
            </nav>
            <div class="main-sidebar sidebar-style-2">
                <aside id="sidebar-wrapper">
                    <div class="sidebar-brand">
                        <a href="/"> <img alt="image" src="/assets/img/AddedSolutions.jpg" class="header-logo" /> <span class="logo-name">tempus</span>
                        </a>
                    </div>
                    <ul class="sidebar-menu">
                        <li class="menu-header">Main</li>

                        <li class="dropdown ">
                            <a href="/dashboard" class="nav-link"><i data-feather="users"></i><span>INSTITUIÇÕES</span></a>
                        </li>
                        <li class="dropdown active">
                            <a href="/dashboard/perfil" class="nav-link"><i data-feather="user"></i><span>PERFIL</span></a>
                        </li>
                    </ul>
                </aside>
            </div>
            <!-- Main Content -->
            <div class="main-content" style="margin-top: -60px;">


                <section class="section">
                    <div class="section-body">
                        <div class="row mt-sm-4">
                            <div class="col-12 col-md-12 col-lg-4">
                                <div class="card author-box">
                                    <div class="card-body">
                                        <div class="author-box-center">
                                            <img alt="image" src="<%= user.foto?user.foto[0].url:'/assets/img/logo.png' %> " class="rounded-circle author-box-picture">
                                            <div class="clearfix"></div>
                                            <div class="author-box-name">
                                                <a href="#">
                                                    <%= user.username %>
                                                </a>
                                            </div>
                                            <div class="author-box-job">
                                                <%= user.nome %>
                                            </div>
                                        </div>
                                        <div class="text-center">
                                            <div class="author-box-description">
                                                <p>
                                                    <%= user.descricao %>
                                                </p>
                                            </div>
                                            <div class="w-100 d-sm-none"></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="card">
                                    <div class="card-header">
                                        <h4>Informações Básica</h4>
                                    </div>
                                    <div class="card-body">
                                        <div class="py-4">

                                            <p class="clearfix">
                                                <span class="float-left">
                            Contacto
                          </span>
                                                <span class="float-right text-muted">
                                                  {{ user.contacto }}
                          </span>
                                            </p>
                                            <p class="clearfix">
                                                <span class="float-left">
                            Email
                          </span>
                                                <span class="float-right text-muted">
                                                  {{user.email }}
                          </span>
                                            </p>
                                            <p class="clearfix">
                                                <span class="float-left">
                            Morada
                          </span>
                                                <span class="float-right text-muted">
                            <a href="#">{{ user.morada}}</a>
                          </span>
                                            </p>
                                            <p class="clearfix">
                                                <span class="float-left">
                            Género
                          </span>
                                                <span class="float-right text-muted">
                           {{user.genero}}
                          </span>
                                            </p>
                                        </div>
                                    </div>
                                </div>

                            </div>
                            <div class="col-12 col-md-12 col-lg-8">
                                <div class="card">
                                    <div class="padding-20">
                                        <ul class="nav nav-tabs" id="myTab2" role="tablist">


                                            <li class="nav-item active">
                                                <a class="nav-link active" @click="selectuser(<%= JSON.stringify(user)%>)" id="profile-tab2" data-toggle="tab" href="#user" role="tab" aria-selected="false">Conta</a>
                                            </li>
                                            <li class="nav-item ml-3" style="float: right;">
                                                <button data-toggle="modal" data-target=".senha" class="btn btn-primary ">Mudar a senha</button>
                                            </li>
                                        </ul>
                                        <div class="tab-content tab-bordered" id="myTab3Content">


                                            <div class="tab-pane show active fade" id="user" role="tabpanel" aria-labelledby="profile-tab2">
                                                <form method="post" id="edit_user">
                                                    <div class="card-header">
                                                        <h4>Editar Perfil</h4>
                                                    </div>


                                                    <div class="card-body">
                                                        <div class="row">
                                                            <div class="form-group col-md-6 col-12">
                                                                <label>Nome</label>
                                                                <input v-model="user.nome" type="text" class="form-control" value="John">
                                                                <div class="invalid-feedback">
                                                                    Por favor, preencha o primeiro nome
                                                                </div>
                                                            </div>
                                                            <div class="form-group col-md-6 col-12">
                                                                <label>Nome do Usuário</label>
                                                                <input v-model="user.username" type="text" class="form-control" value="Deo">
                                                                <div class="invalid-feedback">
                                                                    Por favor, preencha o sobrenome
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="row">
                                                            <div class="form-group col-md-7 col-12">
                                                                <label>Email</label>
                                                                <input v-model="user.email" type="email" class="form-control" value="test@example.com">
                                                                <div class="invalid-feedback">
                                                                    Por favor, preencha o e-mail
                                                                </div>
                                                            </div>
                                                            <div class="form-group col-md-5 col-12">
                                                                <label>Contato</label>
                                                                <input v-model="user.contacto" type="tel" class="form-control" value="">
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label>Foto</label>
                                                            <input name="files" class="form-control" type="file" />
                                                        </div>

                                                        <div class="row">
                                                            <div class="form-group col-9">
                                                                <label>Data Nascimento</label>
                                                                <input type="date" v-model="user.data" class="form-control">
                                                            </div>
                                                            <div class="form-group col-3">
                                                                <label>Género</label>
                                                                <select v-model="user.genero" class="form-control">
                                                                <option>M</option>
                                                                <option>F</option>
                                                              </select>
                                                            </div>

                                                            <button type="submit" class="btn btn-primary w-100">Salvar</button>

                                                        </div>
                                                    </div>
                                                </form>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>


            </div>
            <div class="modal fade senha" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
                <div class="modal-dialog modal-dialog-centered">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="myLargeModalLabel">Senha</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
                        </div>
                        <div class="modal-body">

                            <div class="form-group">
                                <label>Senha atual</label>
                                <input type="password" v-model="senha.password" class="form-control">
                            </div>

                            <div class="form-group">
                                <label>Nova senha</label>
                                <input type="password" v-model="senha.newpassword" class="form-control">
                            </div>

                            <div class="form-group">
                                <label>Confiramação da nova senha</label>
                                <input type="password" v-model="senha.confirmnewpassword" class="form-control">
                            </div>
                            <button @click="alterar" class="btn btn-primary w-100">Alterar</button>


                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- General JS Scripts -->
    <script src="/assets/js/app.min.js"></script>
    <!-- JS Libraies -->
    <script src="/assets/bundles/apexcharts/apexcharts.min.js"></script>
    <!-- Page Specific JS File -->
    <script src="/assets/js/page/index.js"></script>
    <!-- Template JS File -->
    <script src="/assets/js/scripts.js"></script>
    <!-- Custom JS File -->
    <script src="/assets/js/custom.js"></script>

    <script src="/assets/bundles/izitoast/js/iziToast.min.js"></script>

    <script src="/assets/bundles/datatables/datatables.min.js"></script>
    <script src="/assets/bundles/datatables/DataTables-1.10.16/js/dataTables.bootstrap4.min.js"></script>
    <script src="/assets/bundles/jquery-ui/jquery-ui.min.js"></script>
    <!-- Page Specific JS File -->
    <script src="/assets/js/page/datatables.js"></script>

    <script src="/assets/bundles/sweetalert/sweetalert.min.js"></script>
    <!-- Page Specific JS File -->
    <script src="/assets/js/page/sweetalert.js"></script>

    <script src="/assets/js/vue.js"></script>
    <script src="/assets/js/axios.js"></script>
    <script>
        let jwt = "" //'<%= //jwt%>'
        let user = '<%- JSON.stringify(user)%>'
        user = JSON.parse(user)
    </script>
    <script src="/assets/js/tempus/index.perfil.js"></script>


</body>


