    <script>
        let id = '<%= user.intituicao_tecnico.id %>';
        let username = "dffffffffff"




        const uuidutente = uuid.v4()

        let u = {}
        u[uuidutente] = {}
        u[`${uuidutente}_t`] = 0
        u[`${uuidutente}_teste_tempo`] = []
        u[`${uuidutente}_teste`] = 0
        u[`${uuidutente}_errou`] = 0
        u[`${uuidutente}_acertou`] = 0


        let jwt = '<%=jwt %>'

        let user = '<%-JSON.stringify(user)%>'
        console.log(jwt, id, "---", JSON.parse(user), user)
        const socket = io({
            extraHeaders: {
                Authorization: jwt
            }
        });





        socket.on(`bem-vindo:${id}`, (data) => {
            console.log(data)
            iziToast.info({
                title: 'MicroAITec!',
                message: data.message,
                position: 'topRight'
            });
        })
        var app1 = new Vue({
            el: '#app',
            data: {
                uuidutente,
                utentes: [],
                utente: {},
                u,
                observacao: "",
                utente_acertou: 0,
                testes_tempo: [],
                utente_errou: 0,
                utente_teste: 0,
                utente_t: 0,
                ferrameta: 0
            },
            methods: {

                gdesempenho: async(utente) => {
                    // console.log(utente.teste,"sss",myChart.data.datasets[0])

                    app1.u[uuidutente] = utente;
                    app1.u[uuidutente].device = `${app1.u[uuidutente].device}-${uuidutente}`
                    app1.u[`${uuidutente}_t`] = utente.tempo
                    app1.u[`${uuidutente}_teste_tempo`] = []
                    app1.u[`${uuidutente}_teste`] = utente.teste.length
                    app1.u[`${uuidutente}_errou`] = utente.errou
                    app1.u[`${uuidutente}_acertou`] = utente.acertou

                    app1.u[uuidutente].teste.forEach((teste, index) => {

                        app1.u[`${uuidutente}_teste_tempo`].push(teste)

                    });


                },
                obsCreate(event) {
                    event.preventDefault();

                    let obs = {

                        utente: this.utente.id,
                        descricao: this.observacao,


                    }

                    console.log(obs, this.utente.id)
                    axios
                        .post('/api/observacaos', {
                            data: obs,

                        })
                        .then(({
                            data
                        }) => {

                            swal('Observação', 'Ação feita com sucesso!!', 'success').then(() => {

                                $('#add').modal({
                                    show: false,

                                });

                            });


                            console.log(data)
                        })
                        .catch(error => {
                            console.log('An error occurred:', error.response);

                            iziToast.error({
                                title: '3@Mente!',
                                message: "Falha ao adicionar a observação",
                                position: 'topRight'
                            });

                        });


                },
            },
            created: async() => {

                document.getElementById('chat-scroll').classList.remove("d-none");
                document.getElementById('tela').classList.remove("d-none");




            }
        });




        // Message from server

        function init() {
            socket.emit('getUtentes', {
                id
            });
        }

        init()

        socket.on(`add:${id}`, (data) => {
            console.log(data);

            console.log(app1.u)

            let a = false;

            if (data.length > app1.utentes.length) {
                a = true
            }
            app1.utentes = data;

            if (app1.u[uuidutente].id) {

                app1.utentes.map(utente => {

                    if (utente.id == app1.u[uuidutente].id) {
                        app1.u[uuidutente] = utente;
                        app1.u[uuidutente].device = `${app1.u[uuidutente].device}-${uuidutente}`
                        app1.u[`${uuidutente}_t`] = utente.tempo
                        app1.u[`${uuidutente}_teste_tempo`] = []
                        app1.u[`${uuidutente}_teste`] = utente.teste.length
                        app1.u[`${uuidutente}_errou`] = utente.errou
                        app1.u[`${uuidutente}_acertou`] = utente.acertou

                        app1.u[uuidutente].teste.forEach((teste, index) => {

                            app1.u[`${uuidutente}_teste_tempo`].push(teste)

                        });
                    }
                })
            }

            if (a) {
                iziToast.info({
                    title: '3@Mente!',
                    message: app1.utentes.length > 0 ? `${app1.utentes.length} utentes activo no momento` : 'Nenhum utente activo no momento',
                    position: 'topRight'
                });


            }



        });

        console.log(socket, "fffff")

        socket.on(`adds:${id}`, (data) => {
            console.log(data);
            app1.utentes = data;

        });


        socket.on(`addutente:${id}`, (data) => {
            console.log(data);
            app1.utentes.push(data);

        });

        socket.on(`tempo`, () => {
            console.log("dddddd")

            socket.emit('getUtentes', {
                id
            });


        });

        socket.on(`edit:${id}`, (data, d) => {


            if (d == "fechar") {

                if (app1.u[uuidutente].device == `${data.device}-${uuidutente}`) {
                    app1.u[uuidutente] = {};
                    app1.u[`${uuidutente}_t`] = 0
                    app1.u[`${uuidutente}_teste_tempo`] = []
                    app1.u[`${uuidutente}_teste`] = 0
                    app1.u[`${uuidutente}_errou`] = 0
                    app1.u[`${uuidutente}_acertou`] = 0
                }


                let utentes = []

                app1.utentes.map((u) => {

                    console.log(u, data)

                    if (u.device != `${data.device}-${uuidutente}`) {
                        utentes.push(u)
                    }

                })

                app1.utentes = utentes

                iziToast.info({
                    title: '3@Mente!',
                    message: `O utente ${data.nome} terminou de fazer o teste!`,
                    position: 'topRight'
                });

            }

            app1.utentes.map((u) => {






                if (`${data.device}-${uuidutente}` == app1.u[uuidutente].device) {

                    if (d == "fechar") {
                        app1.u[uuidutente] = {};
                        app1.u[`${uuidutente}_t`] = 0
                        app1.u[`${uuidutente}_teste_tempo`] = []
                        app1.u[`${uuidutente}_teste`] = 0
                        app1.u[`${uuidutente}_errou`] = 0
                        app1.u[`${uuidutente}_acertou`] = 0


                    } else {

                        app1.u[uuidutente] = data;
                        app1.u[uuidutente].device = `${app1.u[uuidutente].device}-${uuidutente}`
                        app1.u[`${uuidutente}_t`] = data.tempo
                        app1.u[`${uuidutente}_teste_tempo`] = []
                        app1.u[`${uuidutente}_teste`] = data.teste.length
                        app1.u[`${uuidutente}_errou`] = data.errou
                        app1.u[`${uuidutente}_acertou`] = data.acertou

                        app1.u[uuidutente].teste.forEach((teste, index) => {

                            app1.u[`${uuidutente}_teste_tempo`].push(teste)

                        });

                    }

                }
            })


        });

        var table = $('#tecnicos').DataTable({

            lengthMenu: [5, 10, 20, 50],
            "oLanguage": {
                "sSearch": "Buscar"
            }
        });

        $('#tecnicos tbody').on('click', 'tr', function() {
            $(this).toggleClass('selected');

            $('#exampleModal').modal({
                show: true,

            });
        });
    </script>
