                            </ul>
                        </li>




                    </ul>
                </aside>
            </div>
            <!-- Main Content -->
            <div class="main-content" style="margin-top: -30px;">

                <% if(user.intituicao_tecnico.alertas.length){ %>
                    <section class="section">



                        <div class="card">
                            <div class="card-header" style="display: flex;justify-content: space-between;align-items: center;">
                                <h4>
                                    Lista dos utentes em alertas

                                </h4>
                                <div class="badges">
                                    <span class="badge badge-info mr-3"><%= user.intituicao_tecnico.alertas[0].tempo_start.data.split("T")[0] %></span>
                                    <span class="badge badge-info"><%= user.intituicao_tecnico.alertas[0].tempo_end.data.split("T")[0] %></span>
                                </div>
                            </div>
                            <div class="card-body">
                                <div class="table-responsive">
                                    <table class="table table-striped table-hover" id="tecnicos" style="width:100%;">
                                        <thead>
                                            <tr>
                                                <th>Foto</th>
                                                <th>Nome</th>
                                                <th>Características</th>
                                                <th>Géneros</th>
                                                <th>Idade</th>

                                            </tr>
                                        </thead>
                                        <tbody>

                                            <% user.intituicao_tecnico.alertas[0].utentes.forEach(function(utente) { %>
                                                <tr @click="selet(<%= JSON.stringify(utente)%>,<%= JSON.stringify(user.intituicao_tecnico.alertas[0].testes)%>)">
                                                    <td>
                                                        <img style="width: 30px;height: 30px;" alt="image" src="<%= utente.img?utente.img[0].url:'/assets/img/logo.png' %> " class="user-img-radious-style">
                                                    </td>
                                                    <td>
                                                        <%= utente.nome %>
                                                    </td>
                                                    <td>
                                                        <%= utente.observacao %>
                                                    </td>
                                                    <td>
                                                        <%= utente.sexo %>
                                                    </td>
                                                    <td>
                                                        <%= utente.data?`${(ano-parseInt(utente.data.split('-')[0]))} anos`:utente.data %>
                                                    </td>

                                                </tr>

                                                <% }); %>





                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>

                    </section>

                    <% } else{ %>
                        <h1 class="text-center">Não há nenhuma alerta!</h1>
                        <% } %>



                            <div class="modal fade bd-example-modal-lg" id="ver" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
                                <div class="modal-dialog modal-lg">
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <div style="display: flex; justify-content: space-between; align-items: center;">
                                                <h5 class="modal-title" id="formModal">Utente: {{utente.nome}}</h5>
                                                <a style="float: right;" v-bind:href="'/instituicao/utentes/'+ utente.id" class="btn btn-outline-primary ml-5">Ver o perfil</a>
                                            </div>
                                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                              <span aria-hidden="true">&times;</span>
                            </button>
                                        </div>
                                        <div class="modal-body">
                                            <div class="padding-20">
                                                <ul class="chat-list list-unstyled m-b-0">
                                                    <li>
                                                        <div class="about">
                                                            <div class="row mb-0">
                                                                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
                                                                    <div class="list-inline text-center">
                                                                        <img :src="utente.img?utente.img[0].url:'/assets/img/logo.png'" style="width: 50px; height: 50px;border-radius: 50%;" alt="avatar">
                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
                                                                    <div class="list-inline text-center">
                                                                        <div class="list-inline-item p-r-1"><i data-feather="watch" class="col-green"></i>
                                                                            <h5 class="m-b-0">{{utente.tempo}} min</h5>
                                                                            <p class="text-muted font-14 m-b-0">Tempo Total</p>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
                                                                    <div class="list-inline text-center">
                                                                        <div class="list-inline-item p-r-1"><i data-feather="layers" class="col-green"></i>
                                                                            <h5 class="m-b-0">{{parseFloat(utente.desempenho).toFixed(0)}}%</h5>
                                                                            <p class="text-muted font-14 m-b-0">Taxa de acerto</p>
                                                                        </div>
                                                                    </div>
                                                                </div>

                                                            </div>
                                                        </div>
                                                    </li>

                                                </ul>
                                                <table class="table table-striped table-hover" id="utente" style="width:100%;">
                                                    <thead>
                                                        <tr>

                                                            <th>Ferramenta</th>
                                                            <th>Tempo e Taxa de acerto </th>
                                                            <th>Nº de jogadas</th>
                                                            <th>Data</th>

                                                        </tr>
                                                    </thead>
                                                    <tbody>

                                                    </tbody>



                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>


            </div>

        </div>
    </div>
    <!-- General JS Scripts -->
    <script src="/assets/js/app.min.js"></script>
