    <!-- Template JS File -->
    <script src="/assets/js/scripts.js"></script>
    <!-- Custom JS File -->
    <script src="/assets/js/custom.js"></script>

    <script src="/assets/bundles/datatables/datatables.min.js"></script>
    <script src="/assets/bundles/datatables/DataTables-1.10.16/js/dataTables.bootstrap4.min.js"></script>
    <script src="/assets/bundles/jquery-ui/jquery-ui.min.js"></script>
    <!-- Page Specific JS File -->
    <script src="/assets/js/page/datatables.js"></script>

    <script src="/assets/bundles/sweetalert/sweetalert.min.js"></script>
    <!-- Page Specific JS File -->
    <script src="/assets/js/page/sweetalert.js"></script>

    <script src="/assets/js/vue.js"></script>
    <script src="/assets/js/axios.js"></script>

    <script>
        let t;
        $(document).ready(function() {
            t = $('#utente').DataTable({

                lengthMenu: [10, 20, 30, 40, 50, 100, 500],
                "oLanguage": {
                    "sSearch": "Buscar"
                }
            });

        });
        let id_instituicao = '<%= user.intituicao_tecnico.id %>'

        const app = Vue.createApp({
            data() {
                return {

                    utente: {

                    }

                }
            },
            methods: {
                selet(utente, testes) {


                    this.utente = utente
                    this.utente.testes = []
                    this.utente.tempo = 0;
                    this.utente.desempenho = 0;

                    testes.map(teste => {
                        if (teste.utente.id == utente.id) {
                            delete teste.utente

                            console.log("ddd")

                            this.utente.tempo += teste.tempo
                            this.utente.desempenho += teste.p_acerto;
                            this.utente.testes.push(teste)
                            t.row.add([
                                teste.f,
                                `${teste.tempo} min - ${teste.p_acerto.toFixed(2)}%`,
                                teste.nteste,
                                teste.createdAt.split("T")[0]




                            ]).draw(false);
                        }

                    })


                    this.utente.desempenho /= this.utente.testes.length;

                    console.log(utente)


                },
            },
        }).mount('#app');




        var table = $('#tecnicos').DataTable({

            lengthMenu: [10, 20, 30, 40, 50],
            "oLanguage": {
                "sSearch": "Buscar"
            }
        });

        $('#tecnicos tbody').on('click', 'tr', function() {
            $(this).toggleClass('selected');

            $('#ver').modal({
                show: true,

            });
        });




        $('#utente tbody').on('click', 'tr', function() {
            $(this).toggleClass('selected');


        });
    </script>
</body>


